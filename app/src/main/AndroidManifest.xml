        <activity android:name=".business.factory.FactoryActivity"></activity>
        <activity android:name=".business.abstractfactory.AbstractFactoryActivity"></activity>
        <activity android:name=".business.rxjava.RxjavaActivity"></activity>
        <activity android:name=".business.thread.ThreadSecurityActivity"></activity>
        <activity android:name=".business.doodle.DoodleActivity"></activity>
        <activity android:name=".business.pcm.PcmConvertActivity"></activity>
